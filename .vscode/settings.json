{
  "Lua.runtime.version": "Lua 5.2",
  "Lua.runtime.plugin": ".vscode/sumneko_lua_plugin/plugin.lua",
  "Lua.runtime.pathStrict": true,
  "Lua.runtime.path": [
    "?.lua",
    "src/?.lua",
    "scripts/?.lua",
    "debugging/?.lua",
    "tests/?.lua",
    "tests/lib/test_framework/?.lua",
  ],
  "Lua.diagnostics.globals": [
    "commands",
    "script",
    "game",
  ],
  "Lua.workspace.ignoreDir": [
    ".vscode",
    "debugging/minimal-no-base-mod",
    "debugging/JanSharpDevEnv",
  ],
  "cSpell.flagWords": [
    "colum",
  ],
  "cSpell.ignorePaths": [
    // default:
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".vscode-insiders",
    // custom:
    "**/serpent.lua",
    "LICENSE.txt",
    "LICENSE_THIRD_PARTY.txt",
    "scripts/markdown.lua",
    "debugging/minimal-no-base-mod",
    "debugging/JanSharpDevEnv",
  ],
  "cSpell.words": [
    "binop",
    "binops",
    "breakstat",
    "bytecode",
    "chdir",
    "concat",
    "concats",
    "consts",
    "currentdir",
    "cyclomatic",
    "dostat",
    "elseblock",
    "endianness",
    "eval",
    "evals",
    "extraarg",
    "factorio",
    "forlist",
    "forloop",
    "fornum",
    "forprep",
    "funcs",
    "funcstat",
    "functiondef",
    "functiondefs",
    "getmetatable",
    "gettabup",
    "getupval",
    "gotos",
    "gotostat",
    "ifstat",
    "iife",
    "insts",
    "justarandomgeek",
    "keyallow",
    "keyignore",
    "kvps",
    "loadbool",
    "loadk",
    "loadkx",
    "loadnil",
    "localfunc",
    "localstat",
    "loopstat",
    "lshift",
    "lua's",
    "maxlength",
    "maxlevel",
    "maxnum",
    "metatable",
    "metatables",
    "metatostring",
    "mkdir",
    "nohuge",
    "numformat",
    "paren",
    "prefetch",
    "prepending",
    "prio",
    "proto",
    "protos",
    "ptrs",
    "rawget",
    "rawset",
    "regs",
    "repeatstat",
    "retstat",
    "retstats",
    "rshift",
    "sandboxed",
    "selfcall",
    "setlist",
    "setmode",
    "settabup",
    "setupval",
    "sortkeys",
    "Sumneko",
    "symlinkattributes",
    "tailcall",
    "tailcalls",
    "testblock",
    "testblocks",
    "testset",
    "tforcall",
    "tforloop",
    "Therenas",
    "tostring",
    "tutil",
    "unop",
    "unoptimized",
    "upval",
    "upvals",
    "upvalue",
    "upvalues",
    "userdata",
    "valignore",
    "valtypeignore",
    "vararg",
    "varargs",
    "whilestat"
  ],
  "cSpell.enableFiletypes": [
    "factorio-changelog"
  ],
  "todo-tree.filtering.includeHiddenFiles": true,
  "todo-tree.filtering.excludeGlobs": [
    // default
    "**/node_modules",
    // custom
    "**/debugging/minimal-no-base-mod",
  ],
  "editor.rulers": [110],
}